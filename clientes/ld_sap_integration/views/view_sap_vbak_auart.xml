<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="action_sap_vbak_auart" model="ir.actions.act_window">
        <field name="name">Document Type</field>
        <field name="res_model">sap.vbak.auart</field>
        <field name="view_mode">tree,form</field>
        <field name="help" type="html">
            <p>SAP Document Types</p>
        </field>
    </record>

    <record id="sap_vbak_auart_view_form" model="ir.ui.view">
        <field name="name">sap_vbak_auart.view.form</field>
        <field name="model">sap.vbak.auart</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <group>
                            <field name="code"/>
                            <field name="name"/>
                            <field name="type"/>
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="sap_vba_auart_view_tree" model="ir.ui.view">
        <field name="name">sap.vbak.auart.view.tree</field>
        <field name="model">sap.vbak.auart</field>
        <field name="arch" type="xml">
            <tree string="Document Type">
                <field name="code"/>
                <field name="name"/>
                <field name="type"/>
            </tree>
        </field>
    </record>

</odoo>